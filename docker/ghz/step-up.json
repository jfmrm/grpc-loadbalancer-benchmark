{
    "proto": "helloworld.proto",
    "call": "HelloService.SayHello",
    "concurrency": 10,
    "async": true,
    "load-schedule": "step",
    "load-start": 500,
    "load-step": 500,
    "load-step-duration": "2m",
    "data": {
        "name": "{{.WorkerID}}"
    },
    "metadata": {
        "foo": "bar",
        "trace_id": "{{.RequestNumber}}",
        "timestamp": "{{.TimestampUnix}}"
    },
    "import-paths": [
        "/helloworld"
    ],
    "max-duration": "6m",
    "insecure": true
}
